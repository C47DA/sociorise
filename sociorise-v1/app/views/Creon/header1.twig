    padding : 4px;
    border-radius: 4px;
    margin-left:-10px;
}
.order-success-card .order-details .quantity {
    float:right;
    background-color:rgba(224, 20, 76,0.2);
    padding : 4px;
    border-radius: 4px;
    margin-right:-10px;
}
.order-success-card .user-details {
padding:10px;
background: rgba(111, 214, 73,0.2);
margin-top:25px;
border-radius: 4px;
}

