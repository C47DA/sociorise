    width:100px;
    margin-top: -50px;
    border-radius:50%;
    box-shadow : 0 2px 5px rgba(0,0,0,0.2);
}
.order-success-card h2 {
    font-size: 20px;
    font-weight:500;
    margin : 30px 0 10px;
    text-align:center;
    font-family: 'Lobster', cursive;
    
}
.order-success-card button {
    width:100%;
    margin-top:20px;
    padding:10px 0;
    background: #6fd649;
    outline:none;
    border:0;
    border-radius:6px;
    font-size:18px;
    cursor:pointer;
    box-shadow : 0 5px 5px rgba(0,0,0,0.2);
}
.order-success-card .order-details p {
    padding:10px;
}
